{
  "name": "back-office",
  "version": "1.0.0",
  "private": true,
  "description": "initial project",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "setupFiles": [
    "./src/setupTests.js"
  ],
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.9",
    "connected-react-router": "^6.2.2",
    "fast-memoize": "^2.5.1",
    "gh-pages": "^1.0.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.0.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^1.0.0",
    "styled-components": "3.0.2",
    "superagent": "^4.1.0",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint.js",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{*{js,jsx,json},babelrc,eslintrc,prettierrc,stylelintrc}'",
    "postlint-styles-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{css,scss}'"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "cross-env NODE_ENV=development eslint --cache --format=pretty --fix",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "stylelint --ignore-path .eslintignore --syntax scss --fix",
      "git add"
    ],
    "*.{yml,md}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "husky": "^1.3.1",
    "jest-styled-components": "^6.3.1",
    "jss": "10.0.0-alpha.3",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.7.0",
    "redux-mock-store": "^1.5.3",
    "stylelint": "^9.1.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
